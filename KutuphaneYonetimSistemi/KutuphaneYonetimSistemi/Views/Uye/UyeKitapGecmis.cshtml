@using KutuphaneYonetimSistemi.Models.Entity
@model List<TBLHAREKET>
@{
    ViewBag.Title = "UyeKitapGecmis";
    Layout = "~/Views/Shared/Layout.cshtml";
}
<br />
<h2>Üye: @ViewBag.u1</h2>
<br />
<table class="table table-danger">
    <tr>
        <th>Kitap Adı</th>
        <th>Personel</th>
        <th>Alış Tarihi</th>
        <th>İade Tarihi</th>
    </tr>
    @foreach(var x in Model)
            {
                <tr>
                    <td>@x.TBLKİTAP.AD</td>
                    <td>@x.TBLPERSONEL.PERSONEL</td>
                    <td>@Convert.ToDateTime(x.ALISTARİH).ToString("dd/MM/yyyy")</td>
                    <td>@Convert.ToDateTime(x.UYEGETİRTARİH).ToString("dd/MM/yyyy")</td>
                </tr>
            }
</table>

