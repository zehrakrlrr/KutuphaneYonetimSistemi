@using KutuphaneYonetimSistemi.Models.Entity
@model List<TBLMESAJLAR>
@{
    ViewBag.Title = "Giden";
    Layout = "~/Views/Shared/_LayoutPanelim.cshtml";
}

<h2>Giden Mesajlar Sayfası</h2>
<br />
<table class="table table-bordered">
    <thead>
        <tr>
            <th>Alıcı</th>
            <th>Konu</th>
            <th>Mesaj</th>
            <th>Tarih</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var x in Model)
        {
            <tr>
                <td>@x.ALICI</td>
                <td>@x.KONU</td>
                <td>@x.ICERIK</td>
                <td>@x.TARİH</td>
            </tr>
        }
    </tbody>
</table>

