@using KutuphaneYonetimSistemi.Models.Entity
@model List<TBLHAREKET>

@{
    ViewBag.Title = "Kitaplarım";
    Layout = "~/Views/Shared/_LayoutPanelim.cshtml";
}
<br />
<h2>Kitap Geçmişim</h2>
<br />

<table class="table table-bordered">
    <tr>

        <th>Kitap Adı</th>

        <th>Alış Tarihi</th>
        <th> Üye İade Tarihi</th>

     

    </tr>
    @foreach (var k in Model)
        {
            <tr>
               
                <td>@k.TBLKİTAP.AD</td>
                <td>@k.TBLPERSONEL.PERSONEL</td>
                <td>@Convert.ToDateTime(k.ALISTARİH).ToString("dd/MM/yyyy")</td>
                <td>@Convert.ToDateTime(k.UYEGETİRTARİH).ToString("dd/MM/yyyy")</td>

                

            </tr>
        }
</table>

